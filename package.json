{
  "name": "next13-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "yarn test:unit --coverage",
    "test:watchAll:coverage": "jest --watchAll --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:pnp": "node ./scripts/cypress-pnp.cjs",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "postinstall": "husky install && chmod ug+x .husky/*"
  },
  "dependencies": {
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "beta",
    "styled-reset": "^4.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ducanh2912/next-pwa": "^8.2.0",
    "@next/eslint-plugin-next": "^13.3.0",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/jest-plugin-context": "^2.9.5",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.7.0",
    "eslint": "8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "given2": "^2.1.7",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-plugin-context": "^2.9.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.2.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.0",
  "dependenciesMeta": {
    "next@13.3.0": {
      "unplugged": true
    }
  }
